{
  "name": "cf-msgrouter",
  "version": "0.0.2",
  "private": false,
  "scripts": {
    "build": "tsup src/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.1",
    "@cloudflare/workers-types": "^4.20240620.0",
    "@types/jsonpath": "^0.2.4",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "1.3.0"
  },
  "dependencies": {
    "cf-msgrouter": "link:",
    "jsonpath": "^1.1.1",
    "zod": "^3.23.8"
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "splitting": true,
    "sourcemap": true,
    "format": ["esm", "cjs"],
    "dts": true
  },
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}
